<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background">
        <shape>
            <padding
                android:bottom="0dip"
                android:top="0dip"
                android:left="0dip"
                android:right="0dip"/>
            <corners android:radius="1dp" android:topRightRadius="@dimen/_25dp" android:bottomRightRadius="@dimen/_25dp"/>
            <gradient
                android:startColor="#4E4E4E"
                android:centerColor="#4E4E4E"
                android:centerY="0.50"
                android:endColor="#4E4E4E"
                android:angle="270" />
        </shape>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1dp" android:topRightRadius="@dimen/_25dp" android:bottomRightRadius="@dimen/_25dp"/>
                <gradient
                    android:startColor="#0B9621"
                    android:endColor="#0B9621"
                    android:angle="90" />
            </shape>
        </clip>
    </item>
</layer-list>