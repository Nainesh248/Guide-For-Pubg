                android:layout_marginTop="@dimen/_5dp"
                android:textColor="@color/appcolor"
                android:textStyle="bold"
                android:layout_marginBottom="@dimen/_5dp"
                android:textSize="20dp" />
            <TableRow
                android:id="@+id/fiveRow"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/_100dp"
                android:layout_marginTop="@dimen/_5dp"
                >
                <RelativeLayout
                    android:id="@+id/rl_10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <TextView
                        android:id="@+id/damage1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="TYPE"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_5dp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <TextView
                        android:id="@+id/damage2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="TYPE"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_5dp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20dp" />
                </RelativeLayout>

            </TableRow>
            <TextView
                android:id="@+id/txt_four"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="BULLET SPEED"
                android:gravity="center"
                android:layout_marginTop="@dimen/_5dp"
                android:textColor="@color/appcolor"
                android:layout_marginBottom="@dimen/_5dp"
                android:textStyle="bold"
                android:textSize="20dp" />
            <TableRow
                android:id="@+id/sixRow"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/_100dp"
                android:layout_marginTop="@dimen/_5dp"
                >
                <RelativeLayout
                    android:id="@+id/rl_12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <TextView
                        android:id="@+id/bullet1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="TYPE"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_5dp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <TextView
                        android:id="@+id/bullet2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="TYPE"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_5dp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20dp" />
                </RelativeLayout>

            </TableRow>
            <TextView
                android:id="@+id/txt_five"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="TIME BETWEEN"
                android:gravity="center"
                android:layout_marginTop="@dimen/_5dp"
                android:textColor="@color/appcolor"
                android:layout_marginBottom="@dimen/_5dp"
                android:textStyle="bold"
                android:textSize="20dp" />
            <TableRow
                android:id="@+id/sevenRow"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/_100dp"
                android:layout_marginTop="@dimen/_5dp"
                >
                <RelativeLayout
                    android:id="@+id/rl_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <TextView
                        android:id="@+id/time1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="TYPE"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_5dp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <TextView
                        android:id="@+id/time2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="TYPE"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_5dp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20dp" />
                </RelativeLayout>

            </TableRow>
            <TextView
                android:id="@+id/txt_six"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="NORMAL RELOAD"
                android:gravity="center"
                android:layout_marginBottom="@dimen/_5dp"
                android:layout_marginTop="@dimen/_5dp"
                android:textColor="@color/appcolor"
                android:textStyle="bold"
                android:textSize="20dp" />
            <TableRow
                android:id="@+id/eightRow"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/_100dp"
                android:layout_marginTop="@dimen/_5dp"
                >
                <RelativeLayout
                    android:id="@+id/rl_16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <TextView
                        android:id="@+id/normalreload1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="TYPE"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_5dp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <TextView
                        android:id="@+id/normalreload2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="TYPE"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_5dp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20dp" />
                </RelativeLayout>

            </TableRow>

            <TextView
                android:id="@+id/txt_nine"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="NORMAL SOUND"
                android:gravity="center"
                android:layout_marginTop="@dimen/_5dp"
                android:layout_marginBottom="@dimen/_5dp"
                android:textColor="@color/appcolor"
                android:textStyle="bold"
                android:textSize="20dp" />
            <TableRow
                android:id="@+id/nineRow"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/_100dp"
                android:layout_marginTop="@dimen/_5dp"
                >
                <RelativeLayout
                    android:id="@+id/rl_18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <ImageView
                        android:layout_width="@dimen/_80dp"
                        android:layout_height="@dimen/_80dp"
                        android:background="@drawable/item_details"
                        android:src="@drawable/ic_play"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_5dp"
                        android:padding="@dimen/_15dp"
                        android:id="@+id/normalsound1"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="@dimen/_2dp">
                    <ImageView
                        android:layout_width="@dimen/_80dp"
                        android:layout_height="@dimen/_80dp"
                        android:background="@drawable/item_details"
                        android:src="@drawable/ic_play"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_5dp"
                        android:padding="@dimen/_15dp"
                        android:id="@+id/normalsound2"
                        />
                </RelativeLayout>

            </TableRow>
            <TextView
                android:id="@+id/txt_ten"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="@dimen/_5dp"
