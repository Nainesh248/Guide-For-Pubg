<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/all_background"
    >

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:id="@+id/relative1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/trasparentblack"
                android:layout_margin="@dimen/_10dp">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/round_border_result">
                    <TextView
                        android:id="@+id/title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_margin="10dp"
                        android:gravity="center"
                        android:text="COMPANSETOR(DMR,SR)"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/detail1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/title"
                        android:layout_marginLeft="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:gravity="center"
                        android:text="@string/tips_detail"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_15dp" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relative2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relative1"
                android:layout_margin="@dimen/_10dp"
                android:background="@color/trasparentblack">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/round_border_result">
                    <TextView
                        android:id="@+id/title2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_margin="10dp"
                        android:gravity="center"
                        android:text="COMPANSETOR(DMR,SR)"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/detail2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/title2"
                        android:layout_marginLeft="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:gravity="center"
                        android:text="@string/tips_detail"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_15dp" />
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ads_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10dp"
                android:layout_below="@+id/relative2">
                <com.facebook.ads.NativeAdLayout
                    android:id="@+id/native_ad_container"
                    android:layout_width="match_parent"
                    android:layout_marginTop="@dimen/_10dp"
                    android:visibility="gone"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" />
                <com.google.android.ads.nativetemplates.TemplateView
                    android:visibility= "gone"
                    android:id= "@+id/nativeTemplateView"
                    app:gnt_template_type="@layout/gnt_small_template_view"
                    android:layout_width= "match_parent"
                    android:layout_height= "wrap_content" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/relative3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ads_layout"
                android:layout_margin="@dimen/_10dp"
                android:background="@color/trasparentblack">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/round_border_result">
                    <TextView
                        android:id="@+id/title3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_margin="10dp"
                        android:gravity="center"
                        android:text="COMPANSETOR(DMR,SR)"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/detail3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/title3"
                        android:layout_marginLeft="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:gravity="center"
                        android:text="@string/tips_detail"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_15dp" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relative4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relative3"
                android:layout_margin="@dimen/_10dp"
                android:background="@color/trasparentblack">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/round_border_result">
                    <TextView
                        android:id="@+id/title4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_margin="10dp"
                        android:gravity="center"
                        android:text="COMPANSETOR(DMR,SR)"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/detail4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/title4"
                        android:layout_marginLeft="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:gravity="center"
                        android:text="@string/tips_detail"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_15dp" />
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ads_layout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10dp"
                android:layout_below="@+id/relative4">
                <com.facebook.ads.NativeAdLayout
                    android:id="@+id/native_ad_container1"
                    android:layout_width="match_parent"
                    android:layout_marginTop="@dimen/_10dp"
                    android:visibility="gone"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" />
                <com.google.android.ads.nativetemplates.TemplateView
                    android:visibility= "gone"
                    android:id= "@+id/nativeTemplateView1"
                    app:gnt_template_type="@layout/gnt_small_template_view"
                    android:layout_width= "match_parent"
                    android:layout_height= "wrap_content" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/relative5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ads_layout1"
                android:layout_margin="@dimen/_10dp"
                android:background="@color/trasparentblack">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/round_border_result">
                    <TextView
                        android:id="@+id/title5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_margin="10dp"
                        android:gravity="center"
                        android:text="COMPANSETOR(DMR,SR)"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/detail5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/title5"
                        android:layout_marginLeft="@dimen/_10dp"
                        android:layout_marginRight="@dimen/_10dp"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:gravity="center"
                        android:text="@string/tips_detail"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_15dp" />
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </androidx.core.widget.NestedScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>